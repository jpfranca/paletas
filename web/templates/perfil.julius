$(document).ready(function() {
    var txtTel = $('#frmAlterarDados input[type=text]')[2];
    
    $(txtTel).mask('00 0000-0000');
    $(txtTel).attr('pattern', '.{12,12}');
   
   $('#frmAlterarSenha input[type=password]').each(function (){
      $(this).attr('pattern', '.{4,12}'); 
   });
    
});